<!-- selected-courses.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Selected Courses</title>
</head>
<body>
    <h1>Selected Courses</h1>
    <ul>
        <% selectedCourses.forEach(course => { %>
            <li><%= course.name %></li>
        <% }); %>
    </ul>
</body>
</html>
